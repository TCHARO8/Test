<!doctype html>
<html lang="en" class="light-theme color-header headercolor1">

<head>
    {% include 'block/head.html.twig' %}
    <title>Directions | {% block title %} {% endblock %}</title>
    <link rel="stylesheet" type="text/css" href="@{/assets/css/jquery.mCustomScrollbar.css}"/>
    {% block stylesheets %}

    {% endblock %}

    <script src="https://kit.fontawesome.com/2191a0d00c.js" crossorigin="anonymous"></script>
</head>
<body>
<div class="wrapper">
    {% include 'block/header.html.twig' %}

    {% include 'block/sidebar.html.twig' %}

    <main class="page-content">
        <!--breadcrumb-->
        <div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
            <div class="breadcrumb-title pe-3 text-primary fw-bolder">
                {% block breadcrumb_title %}  {% endblock %}
            </div>
            <div class="ps-3">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb mb-0 p-0">
                        {% if accueil is not defined %}
                            <li class="breadcrumb-item">
                                <a href="javascript:;"><i class="{% block icon_menu %}{% endblock %}"></i></a></li>
                            <li class="breadcrumb-item active" aria-current="page">{% block breadcrumb_subtitle %}{% endblock %}</li>
                        {% endif %}
                    </ol>
                </nav>
            </div>
            {% block settings %}{% include 'block/settings.html.twig' %}{% endblock %}
            {% block buttons %}{% include 'block/buttons.html.twig' %}{% endblock %}
        </div>
        <!--end breadcrumb-->
        {% block content %}{% endblock %}
    </main>
    <div class="overlay nav-toggle-icon"></div>
    <a href="javaScript:;" class="back-to-top"><i class='bx bxs-up-arrow-alt'></i></a>
</div>
</body>
{% include 'block/js.html.twig' %}
{% block javascripts %}{% endblock %}
</html>
